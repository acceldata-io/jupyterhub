{
  "display_name": "PySpark (ODP Spark 3)",
  "language": "python",
  "argv": [
    "__RUNTIME_PREFIX__/bin/__PY__",
    "-m", "ipykernel_launcher",
    "-f", "{connection_file}"
  ],
  "env": {
    "SPARK_HOME": "/usr/odp/__ODP_VERSION__/spark3",
    "PYSPARK_PYTHON": "__RUNTIME_PREFIX__/bin/__PY__",
    "PYSPARK_DRIVER_PYTHON": "__RUNTIME_PREFIX__/bin/__PY__",
    "PYTHONPATH": "/usr/odp/__ODP_VERSION__/spark3/python",
    "PATH": "/usr/odp/__ODP_VERSION__/spark3/bin:/usr/odp/__ODP_VERSION__/spark3/sbin:/usr/bin:/bin"
  }
}
